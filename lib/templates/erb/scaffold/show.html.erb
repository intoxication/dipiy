<%%= render 'base' %>

<div class="toolbar">
  <%%= link_to 'modify', edit_<%= singular_table_name %>_path(@<%= file_name %>), class: "btn btn-sm btn-success" %>
  <%%= link_to 'return', <%= index_helper %>_path, class: "btn btn-sm btn-default" %>
</div>

<div id="<%= singular_table_name %>">
  <% for attribute in attributes -%>
  <p>
    <b><%= attribute.human_name %>:</b>
    <%%= @<%= file_name %>.<%= attribute.name %> %>
  </p>
  <% end -%>
</div>